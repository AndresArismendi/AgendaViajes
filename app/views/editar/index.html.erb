<h1>Otros</h1>

<table class="table table-bordered table-hover table-striped">
  <thead>
  <tr>
    <th scope="col">#</th>
      <th scope="col">Correo</th>
      <th scope="col">Nombre</th>
      <th scope="col">Rut</th>
      <th scope="col">Teléfono</th>
      <th scope="col">Tipo de vehículo</th>
      <th scope="col">usuario</th>
      <th scope="col">Admin</th>     
  </tr>
</thead>
  <% contador = 1 %>
  <% @sessions.each do |session| %>
  <tr>
    <% if session.status == 1 %>
    <td> <%= contador %></td>
      <td><%= session.email %></td>
      <td><%= session.nombre %></td>
      <td><%= session.rut %></td>
      <td><%= session.fono %></td>
      <td><%= session.tipo_vehiculo %></td>
      <td>guía turístico</td>
      <td><%= session.admin %></td>
      <% contador += 1 %>
    <% end %>    
    <!--<td style="width: 1px; white-space: nowrap">-->
      <%#= link_to 'Editar', edit_session_path(session), class: 'btn btn-primary btn-sm' %>
    <!--</td>-->    
  </tr>
  <% end %>
</table>